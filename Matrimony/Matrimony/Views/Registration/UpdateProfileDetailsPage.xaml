<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" NavigationPage.HasNavigationBar="False"
             xmlns:local="clr-namespace:Matrimony.ViewModels"
             x:Class="Matrimony.Views.Registration.UpdateProfileDetailsPage" Title="Add Profile Information" BackgroundColor="White" >
  
    <ContentPage.BindingContext>      
      <local:UpdateProfileViewModel/>
    </ContentPage.BindingContext>
  
    <Grid>
        <ScrollView>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>

                <Grid x:Name="Photo" RowSpacing="0" Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <!--TitleBar-->
                    <StackLayout Orientation="Horizontal" Padding="10,0" BackgroundColor="Blue">
                        <Image Source="back_32px.png"  HeightRequest="25" WidthRequest="25">
                            <Image.GestureRecognizers>
                                <TapGestureRecognizer Tapped="BackArrowTapped"/>
                            </Image.GestureRecognizers>
                        </Image>
                        <Label Text="EDIT PROFILE" Padding="10,0" TextColor="White" VerticalOptions="CenterAndExpand" HorizontalOptions="StartAndExpand"/>
                        <Image Grid.Column="0" Source="Preview.png" Margin="0,0,-5,0" HeightRequest="20" WidthRequest="25" />
                        <Button Text="PREVIEW" Padding="0,0,20,0" BackgroundColor="Transparent" TextColor="White"  FontSize="10"/>
                    </StackLayout>

                    <!--Profile Image with Options-->
                    <Grid  Grid.Row="1" HeightRequest="250">
                        <Image x:Name="mainimage" BackgroundColor="LightGray" Aspect="AspectFit" Source="profile" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
                        <Grid VerticalOptions="EndAndExpand" ColumnSpacing="0" Padding="10,0,10,10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            
                            <StackLayout Grid.Column="0" Orientation="Vertical">
                                <Label Text="Profile Completenes" TextColor="White" 
                                       HorizontalOptions="CenterAndExpand" 
                                       FontSize="Micro" VerticalOptions="CenterAndExpand"/>
                                <ProgressBar BackgroundColor="WhiteSmoke" HeightRequest="20" 
                                             HorizontalOptions="FillAndExpand" 
                                             VerticalOptions="FillAndExpand" 
                                             Progress="{Binding ProfileCompletion}" 
                                             ProgressColor="Green"/>
                            </StackLayout>
                            
                            <StackLayout Grid.Column="1" BackgroundColor="Transparent" Orientation="Vertical">
                                <Image Source="EditContact.png" HeightRequest="20" WidthRequest="30" 
                                       HorizontalOptions="CenterAndExpand" 
                                       VerticalOptions="CenterAndExpand"/>
                                <Label BackgroundColor="Transparent" TextColor="White" 
                                       FontSize="10" Text="EDIT CONTACT" LineBreakMode="WordWrap" 
                                       HorizontalTextAlignment="Center" 
                                       VerticalTextAlignment="Center" 
                                       HorizontalOptions="CenterAndExpand" 
                                       VerticalOptions="CenterAndExpand"/>
                                <StackLayout.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="EditContact_Tapped"/>
                                </StackLayout.GestureRecognizers>
                            </StackLayout>
                            
                            <StackLayout Grid.Column="2" BackgroundColor="Transparent" Orientation="Vertical">
                                <Image Source="AddHoroscope.png" HeightRequest="20" 
                                       BackgroundColor="Transparent" HorizontalOptions="CenterAndExpand" 
                                       VerticalOptions="CenterAndExpand"/>
                                <Label Text="ADD HOROSCOPE" TextColor="White" FontSize="10" LineBreakMode="WordWrap" 
                                       VerticalTextAlignment="Center" HorizontalTextAlignment="Center" 
                                       HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                                <StackLayout.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="AddHoroscope_Tapped"/>
                                </StackLayout.GestureRecognizers>
                            </StackLayout>
                            
                            <StackLayout Grid.Column="3" BackgroundColor="Transparent" Orientation="Vertical">
                                <Image Source="AddPhotos.png" HeightRequest="20" BackgroundColor="Transparent" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                                <Label Text="MANAGE PHOTOS" TextColor="White" FontSize="10" LineBreakMode="WordWrap" VerticalTextAlignment="Center" HorizontalTextAlignment="Center" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                                <StackLayout.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="AddPhotos_Tapped"/>
                                </StackLayout.GestureRecognizers>
                            </StackLayout>
                          
                        </Grid>
                    </Grid>

                </Grid>


                <Grid Padding="10,0" Grid.Row="1">
                    
                    <StackLayout Orientation="Vertical">

                        <!--PERSONAL INFORMATION-->
                        <Label Text="PERSONAL INFORMATION" Padding="10" TextColor="LimeGreen" FontAttributes="Bold" HorizontalOptions="StartAndExpand" VerticalOptions="CenterAndExpand" HorizontalTextAlignment="Start" VerticalTextAlignment="Center" FontSize="15"/>
                        <BoxView BackgroundColor="LightGray" Margin="0,10" HeightRequest="1"/>

                        <!--A few words about my son-->
                        <Grid x:Name="About" Padding="10,0" RowSpacing="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="50"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="50"/>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="0" Source="Fewwords.png" HeightRequest="25" WidthRequest="25" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                                <Label Grid.Column="1" Text="A few words about my son" FontAttributes="Bold" Padding="20,0" TextColor="Black"/>
                                <Image Grid.Column="2" Source="edit.png" HeightRequest="15" WidthRequest="15" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                    <Image.GestureRecognizers>
                                        <TapGestureRecognizer Tapped="Fewwords_Tapped"/>
                                    </Image.GestureRecognizers>
                                </Image>
                            </Grid>
                            <Grid Grid.Row="1" Padding="0,20,0,10">
                                <Label x:Name="Aboutlabel" Text="Browse and pick the XAML pages you want to include in your Xamarin.Forms project directly from Grial UI Kit admin." TextColor="Black"/>
                            </Grid>
                        </Grid>
                        
                        <BoxView BackgroundColor="LightGray" Margin="0,10" HeightRequest="1"/>
                        
                        <!--Basic Details-->
                        <Grid Padding="10,0" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="50"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="50"/>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="0" Source="BasicDetails.png" HeightRequest="25" WidthRequest="25" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                                <Label Grid.Column="1" Text="Basic Details" FontAttributes="Bold" Padding="20,0" TextColor="Black"/>
                                <Image Grid.Column="2" Source="edit.png" HeightRequest="15" WidthRequest="15" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                    <Image.GestureRecognizers>
                                        <TapGestureRecognizer Tapped="BasicDetails_Tapped"/>
                                    </Image.GestureRecognizers>
                                </Image>
                            </Grid>
                          
                            <StackLayout Grid.Row="1" Orientation="Horizontal" Padding="10,20,10,10">
                                <Label Text="Get more responses by verifying your name and age" />
                                <Button BackgroundColor="DarkOrange" FontSize="10" WidthRequest="120" HeightRequest="25" 
                                        Text="Get Identity Badge >" TextColor="White" CornerRadius="25"/>
                            </StackLayout>
                        </Grid>
                        
                        <BoxView BackgroundColor="LightGray" HeightRequest="1" Margin="0,10"/>
                        
                        <!--Basic Details Column-->
                        <Grid Padding="10,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <StackLayout Padding="10,10" Orientation="Vertical">
                                <Label Text="Name" TextColor="Black"/>
                                <Label Text="Age" TextColor="Black"/>
                                <Label Text="Height" TextColor="Black"/>
                                <Label Text="Weight" TextColor="Black"/>
                                <Label Text="Marital Status" TextColor="Black"/>
                                <Label Text="Mother Tongue" TextColor="Black"/>
                                <Label Text="Physical Status" TextColor="Black"/>
                                <Label Text="Body Type" TextColor="Black"/>
                                <Label Text="Complexion" TextColor="Black"/>
                                <Label Text="Profile Created By" TextColor="Black"/>
                                <Label Text="Eating Habits" TextColor="Black"/>
                                <Label Text="Drinking Habits" TextColor="Black"/>
                                <Label Text="Smoking Habits" TextColor="Black"/>
                            </StackLayout>
                            <StackLayout  Padding="10,10" Grid.Column="1" Orientation="Vertical">
                                <Label Text="Name" TextColor="Black"/>
                                <Label Text="Age" TextColor="Black"/>
                                <Label Text="Height" TextColor="Black"/>
                                <Label Text="Weight" TextColor="Black"/>
                                <Label Text="Marital Status" TextColor="Black"/>
                                <Label Text="Mother Tongue" TextColor="Black"/>
                                <Label Text="Physical Status" TextColor="Black"/>
                                <Grid RowSpacing="0">
                                    <Label x:Name="BodTypeLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="BodTypeLabel_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="ComplexionLabel"  Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="Complexion_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Label Text="Profile Created By" TextColor="Black"/>
                                <Grid RowSpacing="0">
                                    <Label x:Name="EatingHabitsLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="EatingHabits_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="DrinkingHabitsLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="DrinkingHabitsLabel_Tapped_" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label Text="Smoking Habits" IsVisible="False" TextColor="Black"/>
                                    <Label x:Name="SmokingHabitsLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="SmokingHabits_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                            </StackLayout>
                        </Grid>
                        
                        <BoxView BackgroundColor="LightGray" HeightRequest="1" Margin="0,10"/>
                        
                        <!--Religious Information-->                       
                            <Grid Padding="10,0" >
                                <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="50"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="50"/>
                                    </Grid.ColumnDefinitions>
                            <Image Grid.Column="0" Source="Religious.png" HeightRequest="25" WidthRequest="25" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                                <Label Grid.Column="1" Text="Religious Information" FontAttributes="Bold" Padding="20,0" TextColor="Black"/>
                            <Image Grid.Column="2" Source="edit.png" HeightRequest="15" WidthRequest="15" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                <Image.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="ReligiousInformation_Tapped"/>
                                </Image.GestureRecognizers>
                            </Image>
                        </Grid>
                       
                        
                        <!--Religious Information details-->
                        <Grid Padding="10,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <StackLayout Padding="10,10" Orientation="Vertical">
                                <Label Text="Religion" TextColor="Black"/>
                                <Label Text="Caste" TextColor="Black"/>
                                <Label Text="Gothra" TextColor="Black"/>
                                <Label Text="Zodiac" TextColor="Black"/>
                                <Label Text="Star" TextColor="Black"/>
                                <Label Text="Raasi" TextColor="Black"/>
                                <Label Text="Dhosham" TextColor="Black"/>
                            </StackLayout>
                            <StackLayout Padding="10,10" Grid.Column="1" Orientation="Vertical">
                                <Label Text="Religion" TextColor="Black"/>
                                <Label Text="Caste" TextColor="Black"/>
                                <Grid RowSpacing="0">
                                    <Label x:Name="GothraLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="GothraLabel_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="ZodiacLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="ZodiacLabel_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="StarLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="StarLabel_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="RaasiLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="RaasiLabel_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="DoshamLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="DoshamLabel_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                            </StackLayout>
                        </Grid>
                        
                        <BoxView BackgroundColor="LightGray" HeightRequest="1" Margin="0,10"/>

                        <!--Professional Information-->
                            <Grid Padding="10,0" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="50"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="50"/>
                                </Grid.ColumnDefinitions>
                            <Image Grid.Column="0" Source="Professional.png" HeightRequest="25" WidthRequest="25" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                                <Label Grid.Column="1" Text="Professional Information" FontAttributes="Bold" Padding="20,0" TextColor="Black"/>
                            <Image Grid.Column="2" Source="edit.png" HeightRequest="15" WidthRequest="15" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                <Image.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="Professional_Tapped"/>
                                </Image.GestureRecognizers>
                            </Image>
                        </Grid>
                         
                        <!--Professional details-->
                        <Grid Padding="10,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <StackLayout Padding="10,10" Orientation="Vertical">
                                <Label Text="Education Category" TextColor="Black"/>
                                <Label Text="College/Institution" TextColor="Black"/>
                                <Label Text="Occupation" TextColor="Black"/>
                                <Label Text="Employed In" TextColor="Black"/>
                            </StackLayout>
                            <StackLayout  Padding="10,10" Grid.Column="1" Orientation="Vertical">
                                <Label Text="Education Category" TextColor="Black"/>
                                <Grid RowSpacing="0">
                                    <Label Text="College/Institution" IsVisible="False" TextColor="Black"/>
                                    <Label Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label Text="Occupation" IsVisible="False" TextColor="Black"/>
                                    <Label Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label Text="Employed In" IsVisible="False" TextColor="Black"/>
                                    <Label Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                            </StackLayout>
                        </Grid>
                        
                        <BoxView BackgroundColor="LightGray" HeightRequest="1" Margin="0,10"/>

                        <!--Location-->
                        <Grid Padding="10,0" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="50"/>
                            </Grid.ColumnDefinitions>
                            <Image Grid.Column="0" Source="Locationnew.png" HeightRequest="25" WidthRequest="25" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                            <Label Grid.Column="1" Text="Location" FontAttributes="Bold" Padding="20,0" TextColor="Black"/>
                            <Image Grid.Column="2" Source="edit.png" HeightRequest="15" WidthRequest="15" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                <Image.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="Location_Tapped"/>
                                </Image.GestureRecognizers>
                            </Image>
                        </Grid>
                        <Grid Padding="10,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <StackLayout Padding="10,10" Orientation="Vertical">
                                <Label Text="Country" TextColor="Black"/>
                                <Label Text="State" TextColor="Black"/>
                                <Label Text="City" TextColor="Black"/>
                                <Label Text="Citizenship" TextColor="Black"/>
                            </StackLayout>
                            <StackLayout Padding="10,10" Grid.Column="1" Orientation="Vertical">
                                <Label Text="Country" TextColor="Black"/>
                                <Label Text="State" TextColor="Black"/>
                                <Label Text="City" TextColor="Black"/>
                                <Label Text="Citizenship" TextColor="Black"/>
                            </StackLayout>
                        </Grid>
                        
                        <BoxView BackgroundColor="LightGray" HeightRequest="1" Margin="0,10"/>

                        <!--Family -->
                        <Grid Padding="10,0" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="50"/>
                            </Grid.ColumnDefinitions>
                            <Image Grid.Column="0" Source="Family.png" HeightRequest="25" WidthRequest="25" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                            <Label Grid.Column="1" Text="Family Details" FontAttributes="Bold" Padding="20,0" TextColor="Black"/>
                            <Image Grid.Column="2" Source="edit.png" HeightRequest="15" WidthRequest="15" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                <Image.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="FamilyDetails_Tapped"/>
                                </Image.GestureRecognizers>
                            </Image>
                        </Grid>

                        <!--Family Details-->
                        <Grid Padding="10,10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <StackLayout Padding="10,10" Orientation="Vertical">
                                <Label Text="Family Values" TextColor="Black"/>
                                <Label Text="Family Type" TextColor="Black"/>
                                <Label Text="Family Status" TextColor="Black"/>
                                <Label Text="Father's Occupation" TextColor="Black"/>
                                <Label Text="Mother's Occupation" TextColor="Black"/>
                                <Label Text="Brother(s)" TextColor="Black"/>
                                <Label Text="Brother(s) Married" TextColor="Black"/>
                                <Label Text="Sister(s)" TextColor="Black"/>
                                <Label Text="Sister(s) Married" TextColor="Black"/>
                                <Label Text="Ancestral Origin" TextColor="Black"/>
                            </StackLayout>
                            <StackLayout Padding="10,10" Grid.Column="1" Orientation="Vertical">
                                <Grid RowSpacing="0">
                                    <Label x:Name="FamilyValuesLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="FamilyValues_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="FamilyTypeLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="FamilyType_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="FamilyStatusLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="FamilyStatus_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="FathersOccupationLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="FathersOccupation_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="MothersOccupationLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="MothersOccupation_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="BrothersLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="Brothers_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="BrothersMarriedLabel"  Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="BrothersMarried_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                                <Grid RowSpacing="0">
                                    <Label x:Name="SistersLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="Sisters_Tapped"/>
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                               
                                <Grid RowSpacing="0">
                                    <Label x:Name="SistersMarriedLabel"  Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="SistersMarried_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>

                                <Grid RowSpacing="0">
                                    <Label Text="Ancestor" IsVisible="False" TextColor="Black"/>
                                    <Label x:Name="AncestorLabel" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                        <Label.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="Ancestor_Tapped" />
                                        </Label.GestureRecognizers>
                                    </Label>
                                </Grid>
                            </StackLayout>
                        </Grid>

                        <BoxView BackgroundColor="LightGray" HeightRequest="1" Margin="0,10"/>

                        <!--About our family-->
                        <Grid Padding="10,10" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="50"/>
                            </Grid.ColumnDefinitions>
                            <Image Grid.Column="0" Source="Aboutourfamily.png" HeightRequest="25" WidthRequest="25" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
                            <Label Grid.Column="1" Text="About our family" FontAttributes="Bold" Padding="20,0" TextColor="Black"/>
                            <Image Grid.Column="2" Source="edit.png" HeightRequest="15" WidthRequest="15" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                                <Image.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="Aboutourfamily_Tapped"/>
                                </Image.GestureRecognizers>
                            </Image>
                        </Grid>
                       
                        <Grid RowSpacing="0" Padding="10,10">
                            <Label x:Name="AboutourfamilyLabel"  Padding="10,0" Text="+ Add Details" IsVisible="True" BackgroundColor="Transparent" TextColor="Red">
                                <Label.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="Aboutourfamily_Tapped" />
                                </Label.GestureRecognizers>
                            </Label>
                        </Grid>

                        <BoxView BackgroundColor="LightGray" HeightRequest="1" Margin="0,10"/>


                        <!--Hobbies and Interest-->
                        <Grid Padding="10,10" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="*"/>
